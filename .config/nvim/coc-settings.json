{
  "python.formatting.provider": "black",
  "explorer.file.showHiddenFiles": true,
  "go.goplsPath": "/bin/gopls",
  "prettier.eslintIntegration": true,
  "coc.source.emoji.enable": true,
  "coc.source.emoji.filetypes": null,
  "coc.source.emoji.triggerCharacters": [
    "~"
  ],
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.ts": "prettier",
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatter.css": "prettier",
  "prettier.disableLanguages": [],
  "eslint.run": "onSave",
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "java.jdt.ls.vmargs": "-javaagent:/home/lucas/Downloads/lombok.jar -Xbootclasspath/a:/home/lucas/Downloads/lombok.jar -noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication",
  "rust-analyzer.server.path": "/bin/rust-analyzer",
  "svelte.enable-ts-plugin": true
}
